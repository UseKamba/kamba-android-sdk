<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.usekamba.kamba_payments_sdk">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kamba_res_ic_launcher"
        android:label="@string/kamba_res_app_name"
        android:roundIcon="@mipmap/kamba_res_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/kamba_res_AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckoutActivity"></activity>
    </application>

</manifest>